<?xml version="1.0" encoding="UTF-8"?>多注释，比较难看，加入下面的配置控制 -->
        <commentGenerator>
            <property name="suppressAllComments" value="false" />
            <property name="suppressDate" value="false" />
        </commentGenerator>

        <!--<property name="prefix" value="wk_"/>-->

           <!-- 数据库连接 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://127.0.0.1:3306/wukong_read?useSSL=false"
                        userId="root"
                        password="rootmysql">
        </jdbcConnection>

        <javaTypeResolver >
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>

        <!-- 数据表对应的model 层  -->
        <javaModelGenerator targetPackage="com.wukong.examples.entity" targetProject="log">
            <property name="enableSubPackages" value="true" />
            <property name="trimStrings" value="true" />
        </javaModelGenerator>

        <!-- sql mapper 隐射配置文件 如果是 MyBatis3DynamicSql 就不生成xml文件 -->
        <sqlMapGenerator targetPackage="com.wukong.examples.mapper"  targetProject="log">
            <property name="enableSubPackages" value="true" />
        </sqlMapGenerator>

        <!-- 在ibatis2 中是dao层，但在mybatis3中，其实就是mapper接口 -->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.wukong.examples.dao"  targetProject="log">
            <property name="enableSubPackages" value="true" />
        </javaClientGenerator>

        <!-- 要对那些数据表进行生成操作，必须要有一个. -->
        <!--<table tableName="user" domainObjectName="user">-->
        <!--</table>-->

        <table tableName="wk_order" >


            <generatedKey column="order_id" sqlStatement="Mysql" identity="true"/>

            <!--去掉前缀，但是现在好像有问题-->
            <domainObjectRenamingRule searchString="wk_" />

        </table>

    </context>
</generatorConfiguration>