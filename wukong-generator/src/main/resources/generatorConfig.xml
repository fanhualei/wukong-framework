<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

    <classPathEntry location="/home/fan/IdeaProjects/wukong-framework/wukong-generator/src/main/resources/mysql-connector-java-5.1.45-bin.jar" />


    <!--MyBatis3DynamicSql  MyBatis3-->
    <context id="wukong-generator" targetRuntime="MyBatis3DynamicSql">

        <plugin type="org.mybatis.generator.plugins.ToStringPlugin"/>
        <plugin type="com.wukong.generator.mybatis.plugins.WukongPlugin"/>

        <commentGenerator>
            <property name="suppressAllComments" value="true" />
            <property name="suppressDate" value="true" />
        </commentGenerator>






        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://127.0.0.1:3306/wukong_read?useSSL=false"
                        userId="root"
                        password="rootmysql">
        </jdbcConnection>

        <javaTypeResolver >
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>

        <!--model-->
        <javaModelGenerator targetPackage="com.wukong.examples.model" targetProject="log">
            <property name="enableSubPackages" value="true" />
            <property name="trimStrings" value="true" />
        </javaModelGenerator>

        <!-- sql mapper xml-->
        <sqlMapGenerator targetPackage="com.wukong.examples.mapper"  targetProject="log">
            <property name="enableSubPackages" value="true" />
        </sqlMapGenerator>

        <!--dao-->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.wukong.examples.dao"  targetProject="log">
            <property name="enableSubPackages" value="true" />
            <property name="prefix" value="wk_"/>
            <property name="ignores" value=""/>
            <property name="targetPackage" value="com.wukong.examples.service"/>
            <property name="targetProject" value="log"/>
            <property name="runWukongRule" value="true"/>
        </javaClientGenerator>


        <table tableName="wk_order" >
            <generatedKey column="order_id" sqlStatement="Mysql" identity="true"/>
            <domainObjectRenamingRule searchString="^wk_" />
        </table>

    </context>
</generatorConfiguration>